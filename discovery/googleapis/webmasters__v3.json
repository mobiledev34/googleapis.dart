                    "path": "sites/{siteUrl}",
                    "scopes": [
                        "https://www.googleapis.com/auth/webmasters"
                    ]
                },
                "get": {
                    "description": "Retrieves information about specific site.",
                    "httpMethod": "GET",
                    "id": "webmasters.sites.get",
                    "parameterOrder": [
                        "siteUrl"
                    ],
                    "parameters": {
                        "siteUrl": {
                            "description": "The site's URL, including protocol, for example 'http://www.example.com/'",
